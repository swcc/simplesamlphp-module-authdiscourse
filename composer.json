{
    "name": "swcc/simplesamlphp-module-authdiscourse",
    "description": "A module that is able to perform authentication against a Discourse server",
    "type": "simplesamlphp-module",
    "keywords": ["simplesamlphp", "discourse"],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Paul Bonaud",
            "email": "paul+gh@bonaud.fr"
        }
    ],
    "config": {
        "preferred-install": {
            "simplesamlphp/simplesamlphp": "source",
            "*": "dist"
        }
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\Module\\authdiscourse\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\Utils\\": "vendor/simplesamlphp/simplesamlphp/tests/Utils"
        }
    },
    "require": {
        "php": ">=7.2",
        "simplesamlphp/composer-module-installer": "~1.0",
        "simplesamlphp/simplesamlphp": "dev-master",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp-test-framework": "^0.1.0"
    },
    "support": {
        "issues": "https://github.com/swcc/simplesamlphp-module-authdiscourse/issues",
        "source": "https://github.com/swcc/simplesamlphp-module-authdiscourse"
    }
}
